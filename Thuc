function stay()
		-- Script generated by SimpleSpy - credits to exx#9394
 
		local args = {
			[1] = workspace.FriendlyNPCs:FindFirstChild("Hair Stylist")
		}
 
		game:GetService("Players").LocalPlayer.Backpack.ServerTraits.ChatStart:FireServer(unpack(args))
		-- Script generated by SimpleSpy - credits to exx#9394
		wait(.3)
		local args = {
			[1] = {
				[1] = "Yes"
			}
		}
 
		game:GetService("Players").LocalPlayer.Backpack.ServerTraits.ChatAdvance:FireServer(unpack(args))
		wait(1.5)
		-- Script generated by SimpleSpy - credits to exx#9394
 
		local args = {
			[1] = "woah"
		}
 
		game:GetService("Players").LocalPlayer.Backpack.HairScript.RemoteEvent:FireServer(unpack(args))
	end
 
	local mouse = game.Players.LocalPlayer:GetMouse()
	mouse.KeyDown:connect(function(key)
		if key == 'n' then
            stay()
		end
	end)

	
function respawnf()
		-- Script generated by SimpleSpy - credits to exx#9394
 
		local args = {
			[1] = workspace.FriendlyNPCs:FindFirstChild("Hair Stylist")
		}
 
		game:GetService("Players").LocalPlayer.Backpack.ServerTraits.ChatStart:FireServer(unpack(args))
		-- Script generated by SimpleSpy - credits to exx#9394
		wait(.3)
		local args = {
			[1] = {
				[1] = "Yes"
			}
		}
 
		game:GetService("Players").LocalPlayer.Backpack.ServerTraits.ChatAdvance:FireServer(unpack(args))
		wait(0.5)
		-- Script generated by SimpleSpy - credits to exx#9394
 
		local args = {
			[1] = "woah"
		}
 
		game:GetService("Players").LocalPlayer.Backpack.HairScript.RemoteEvent:FireServer(unpack(args))
	end
	function respawnf2()
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Thuc";
			Text = "FasterReset";
		})
	end
 
	local mouse = game.Players.LocalPlayer:GetMouse()
	mouse.KeyDown:connect(function(key)
		if key == 'v' then
			respawnf2()
			respawnf()
		end
	end)

	

	function peace()
wait(1)
        loadstring(game:HttpGet('https://raw.githubusercontent.com/trithuc1662000/Secrect/main/Secrect'))()
	end
	function peace2()
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Thuc";
			Text = "Welcome";
		})
	end
	local Player = game.Players.LocalPlayer
	Player.Chatted:connect(function(cht)
		if cht:match("Peace") then
			peace()
			peace2()
		end
	end)

function backspawn()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/trithuc1662000/Back/main/Back'))()
    end
 
	function backspawn2()
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Thuc";
			Text = "BackSpawn";
		})
	end
 
	local mouse = game.Players.LocalPlayer:GetMouse()
	mouse.KeyDown:connect(function(key)
		if key == 'o' then
			backspawn2()
			backspawn()
		end
	end)
 
function godmode()
local Plr = game.Players.LocalPlayer
local GodModeBool = true

game:GetService("RunService").RenderStepped:connect(function()
if  GodModeBool  then
game.Workspace.Touchy.Part.CFrame = Plr.Character.HumanoidRootPart.CFrame + Vector3.new(0,0,1)
if Plr.PlayerGui:FindFirstChild("Popup") then
Plr.PlayerGui.Popup.Enabled = false
end
if not GodModeBool  then
return
end end end)
	end
 
	function godmode2()
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Thuc";
			Text = "GodMode ON ";
		})
	end
	local mouse = game.Players.LocalPlayer:GetMouse()
	mouse.KeyDown:connect(function(key)
		if key == ',' then
			godmode2()
			godmode()
		end
	end)

