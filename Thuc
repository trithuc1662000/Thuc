function respawnf()
		-- Script generated by Thuc
 
		local args = {
			[1] = workspace.FriendlyNPCs:FindFirstChild("Hair Stylist")
		}
 
		game:GetService("Players").LocalPlayer.Backpack.ServerTraits.ChatStart:FireServer(unpack(args))
		-- Script generated by Thuc
		wait(.3)
		local args = {
			[1] = {
				[1] = "Yes"
			}
		}
 
		game:GetService("Players").LocalPlayer.Backpack.ServerTraits.ChatAdvance:FireServer(unpack(args))
		wait(0.5)
		-- Script generated by Thuc 
 
		local args = {
			[1] = "woah"
		}
 
		game:GetService("Players").LocalPlayer.Backpack.HairScript.RemoteEvent:FireServer(unpack(args))
	end
	function respawnf2()
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Daishi";
			Text = "FasterReset";
		})
	end
 
	local mouse = game.Players.LocalPlayer:GetMouse()
	mouse.KeyDown:connect(function(key)
		if key == 'v' then
			respawnf2()
			respawnf()
		end
	end)
  
  on = true
	off = false
	local lplr = game.Players.LocalPlayer
	local mouse = game.Players.LocalPlayer:GetMouse()
	mouse.KeyDown:connect(function(key)
		if key == "[" then
			if _G.anti == false then
				_G.anti = on
				game:GetService("StarterGui"):SetCore("SendNotification", {
					Title = "Un-Glitch Enabled";
					Text = "Press ' to disable.";
				})
			elseif _G.anti == true then
				_G.anti = off
				game:GetService("StarterGui"):SetCore("SendNotification", {
					Title = "Un-Glitch Disabled";
					Text = "Press ; to enable.";
				})
			end
		end
		if key == "]" then
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Anti-Kick";
				Text = "Press > to disable.";
			})
			lplr.Character.Humanoid.Animator:Destroy()
		end
		if key == "." then
			lplr.Character.Humanoid.Health = 0
		end
	end)
 
	_G.anti = false
	_G.anti2 = false
 
	while wait() do
		if _G.anti == true then
			if lplr.Character:WaitForChild("LowerTorso") then
				for i, v in pairs(lplr.Character.LowerTorso:GetChildren()) do
					if v.Name == "Velocity" then
						v:Destroy()
					end
					if v.Name == "KnockBacked" then
						v:Destroy()
					end
					if v.Name == "BodyVelocity" then
						v:Destroy()
					end
				end
			end
		end
	end
	
	mouse.KeyDown:connect(function(key)
if key == "n" then
game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "No-Slow Enabled";
        Text = "Press n to disable.";
        })
_G.Skiznillett = noslow
elseif key == "-" then
game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "No-Slow Disabled";
        Text = "Press n to enable.";
        })
_G.Skiznillett = slow
end
end)
 
_G.Skiznillett = false
 
while wait() do
	if _G.Skiznillett == true then
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
	        if v.Name == "Justice Combination" then
	            local action = game.Players.LocalPlayer.Character:WaitForChild("Action")
	            if action then wait() action:Destroy() end end
	        if v.Name == "Attacking" then
	    		v:Destroy()
	        end
	        if v.Name == "Action" then
	            v:Destroy()
	        end
	        if v.Name == "Killed" then
	            v:Destroy()
	        end
			if v.Name == "Block" and v.Value == true then
				v.Value = false
			end
		end
    end
end
